   <ul>\r\n        <li>Force: {statitiques[0]} / 18\r\n        {(points> 0 && statitiques[0] <18) ?\r\n          <Button variant=\"warning\" size=\"sm\" className=\"m-1\" onClick={()=>handleClick(0)}>incr</Button>\r\n          :null\r\n        }\r\n        </li>\r\n        <li>Intelligence: {statitiques[1]}  / 18\r\n        {(points> 0 && statitiques[1] <18) ?\r\n          <Button variant=\"warning\" size=\"sm\" className=\"m-1\" onClick={()=>handleClick(1)}>incr</Button>\r\n          :null\r\n        }\r\n        </li>\r\n        <li>Dextérité: {statitiques[2]} / 18\r\n        {(points> 0 && statitiques[2] <18) ?\r\n          <Button variant=\"warning\" size=\"sm\" className=\"m-1\" onClick={()=>handleClick(2)}>incr</Button>\r\n          :null\r\n        }\r\n        </li>\r\n        <li>Charisme: {statitiques[3]} / 18\r\n        {(points> 0 && statitiques[3] <18) ?\r\n          <Button variant=\"warning\" size=\"sm\" className=\"m-1\" onClick={()=>handleClick(3)}>incr</Button>\r\n          :null\r\n        }\r\n        </li>\r\n        <li>Endurance: {statitiques[4]} / 18\r\n        {(points> 0 && statitiques[4] <18) ?\r\n          <Button variant=\"warning\" size=\"sm\" className=\"m-1\" onClick={()=>handleClick(4)}>incr</Button>\r\n          :null\r\n        }\r\n        </li>\r\n      </ul>\r\n    </>\r\n  );\r\n}\r\n\r\nfunction ChoisirRace(props) {\r\n  const handleClick=props.handleClick;\r\n  return (\r\n    <>\r\n      <h1>Choisir votre race</h1>\r\n      <p>Cliquez sur l'image</p>\r\n      <Table bordered size=\"sm\" >\r\n        <thead>\r\n          <tr>\r\n            <td align=\"center\">Nain</td>\r\n            <td align=\"center\">Humain</td>\r\n            <td align=\"center\">Elfe</td>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr>\r\n            <td align=\"center\">\r\n              <img src=\"nain.jpg\" height=\"200\" alt=\"nain\" onClick={()=>handleClick(\"nain\")}/>\r\n            </td>\r\n            <td align=\"center\">\r\n              <img src=\"humain.jpg\" height=\"300\" alt=\"humain\" onClick={()=>handleClick(\"humain\")}/>\r\n            </td>\r\n            <td align=\"center\">\r\n              <img src=\"elfe.jpg\" height=\"250\" alt=\"elfe\" onClick={()=>handleClick(\"elfe\")}/>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </Table>\r\n    </>\r\n  );\r\n}\r\n\r\nfunction App() {\r\n\r\n  const [statistiques,setStatistiques]=useState(null);\r\n  const [points,setPoints]=useState(5);\r\n \r\n  function handleClickRace(race){\r\n    var nouvellesStats=[];\r\n    nouvellesStats[0]=GenererNombreAleatoire(6,12);\r\n    nouvellesStats[1]=GenererNombreAleatoire(6,12);\r\n    nouvellesStats[2]=GenererNombreAleatoire(6,12);\r\n    nouvellesStats[3]=GenererNombreAleatoire(6,12);\r\n    nouvellesStats[4]=GenererNombreAleatoire(6,12);\r\n\r\n    if(race===\"nain\"){\r\n\r\n      nouvellesStats[0]+=3;\r\n      nouvellesStats[1]-=2;\r\n      //statistiques[2]+=3;\r\n      nouvellesStats[2]+=3;\r\n      nouvellesStats[3]-=3;\r\n    }\r\n    else if(race===\"elfe\"){\r\n\r\n      nouvellesStats[0]-=2;\r\n      nouvellesStats[1]+=3;\r\n      nouvellesStats[2]+=3;\r\n      nouvellesStats[3]-=2;\r\n    }\r\n\r\n    setStatistiques(nouvellesStats);\r\n  }\r\n\r\n  function handleClickStats(index){\r\n\r\n       var nouvellesStats=statistiques.slice();\r\n       nouvellesStats[index]+=1;\r\n\r\n       setPoints(points - 1);\r\n       setStatistiques(nouvellesStats);\r\n  }\r\n  function GenererNombreAleatoire(min,max){\r\n   return Math.floor(Math.random()* (max - min +1 ))+min;\r\n  }\r\n\r\n  composantAffiche = <ChoisirRace handleClick ={handleClickRace} />;\r\n \r\n\r\n  if (statistiques != null) {\r\n    var composantAffiche = <ChangerStats stats={statistiques} points={points} handleClick={handleClickStats} />;\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <Container>\r\n        <Row>\r\n          <Col>\r\n            {composantAffiche}\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </>\r\n  );\r\n}\r\n\r\nReactDOM.render(\r\n  <App />,\r\n  document.getElementById('root')\r\n);\r\n\r\n\r\nexport default App;",["87","88"],"C:\\Users\\Admin\\Desktop\\Session_Automne_2020\\Application_Bases_De_Données\\Revision_Backend\\revision-backend-reactjs\\revision\\src\\Pages\\PageArticle.js",[],"C:\\Users\\Admin\\Desktop\\Session_Automne_2020\\Application_Bases_De_Données\\Revision_Backend\\revision-backend-reactjs\\revision\\src\\Pages\\ContenusArticles.js",["89"],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":1,"column":8,"nodeType":"96","messageId":"97","endLine":1,"endColumn":13},{"ruleId":"94","severity":1,"message":"98","line":1,"column":16,"nodeType":"96","messageId":"97","endLine":1,"endColumn":24},{"ruleId":"90","replacedBy":"99"},{"ruleId":"92","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":132,"column":3,"nodeType":"96","messageId":"103","endLine":132,"endColumn":19},{"ruleId":"90","replacedBy":"104"},{"ruleId":"92","replacedBy":"105"},{"ruleId":"94","severity":1,"message":"95","line":1,"column":8,"nodeType":"96","messageId":"97","endLine":1,"endColumn":13},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"no-unused-vars","'React' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.",["106"],["107"],"no-use-before-define","'composantAffiche' was used before it was defined.","usedBeforeDefined",["106"],["107"],"no-global-assign","no-unsafe-negation"]